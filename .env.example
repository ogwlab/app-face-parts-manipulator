# Vercel環境変数設定例
# 本番環境では Vercel ダッシュボードで設定

# 環境タイプ
NODE_ENV=production

# アプリケーション設定
VITE_APP_NAME=Face Parts Manipulator
VITE_APP_VERSION=6.1.0
VITE_APP_DESCRIPTION=顔パーツ個別操作アプリ - 高度なファイル名生成システム版

# セキュリティ設定
VITE_SECURITY_MODE=strict
VITE_CSP_MODE=production

# デバッグ設定
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false

# パフォーマンス設定
VITE_ENABLE_SOURCE_MAPS=false
VITE_BUNDLE_ANALYSIS=false

# face-api.js設定
VITE_MODEL_BASE_URL=/models
VITE_FACE_DETECTION_CONFIDENCE=0.5
VITE_LANDMARK_DETECTION_CONFIDENCE=0.5

# Vercel設定
VERCEL_ENV=production
VERCEL_URL=your-app.vercel.app